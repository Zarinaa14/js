<meta charset="utf-8">
</head>
<body>
<script>
var reg=/^[\+]?(375)?(80)?[\-]?[(]?(17|25|29|33|44)[)]?[\- ]?[1-9]\d{2}[\-]?\d{2}[\-]?\d{2}/g
var phone=prompt("Enter the phone number: ");
if(reg.test(phone))
{
	alert("It is a phone number");
	alert( phone.match(reg) );
}
else
{
	alert("Try again");
}

</script>	
</body>
</html> 
