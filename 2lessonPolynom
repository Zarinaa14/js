<!DOCTYPE HTML>
<html>
<head>
<meta charset="utf-8">
</head>
<body>

<script>
var arr = [25, 21, 7, 0, 5, 11];
    var x = prompt("Enter x");
function Polynomial(arr, x){
	var result = 0, 
	
	for(var i = 0; i < arr.length; i++){
		result += arr[i] * Math.pow(x, i);
	}

	      return result;
}

var result = Polynomial(arr, x);
alert("Polynomial = " + result);

</script>
</body>
